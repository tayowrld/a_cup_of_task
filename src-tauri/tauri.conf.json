{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "A Cup of Tasks",
  "mainBinaryName": "a_cup_of_tasks",
  "version": "0.5.5",
  "identifier": "com.saimori.ACupOfTasks",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "A Cup of Tasks",
        "width": 1200,
        "height": 900,
        "minWidth": 950,
        "minHeight": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "linux": {
      "deb": {
        "files": {}
      },
      "rpm": {
        "epoch": 0,
        "files": {},
        "release": "1"
      }
    },
    "icon": [
      "icons/icon.png"
    ]
  }
}
